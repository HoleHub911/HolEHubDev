		_G.OneShot = t
		while _G.OneShot do
			task.wait(0.1)

			local tool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
			if tool and tool:FindFirstChild("MainModule") then
				for _, settingModule in pairs(tool.MainModule:GetChildren()) do
					local asdas = require(settingModule)
					asdas.BaseDamage = {math.huge,math.huge}
				end
			end
		end
	end)


