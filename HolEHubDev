		-----[ Teleport ]-----
	local function Find(name)
		for _,v in pairs(game.workspace:GetDescendants()) do
			if v:IsA("Model") and v.Name == name then
				print(name)
				for _,u in pairs(v:GetDescendants()) do
					if u.Name == "HumanoidRootPart" then
						print('found hrp')
						return u
					end
				end
			
			end
		end
	end

	local function FindPromt(name)
		for _,v in pairs(game.workspace:GetDescendants()) do
			if v:IsA("Model") and v.Name == name then
				for _,u in pairs(v:GetDescendants()) do
					if u:IsA("ProximityPrompt") then
						return u
					end
				end
			end
		end
	end
	local function Buy(name)
		local olspos = game.Players.LocalPlayer.HumanoidRootPart.CFrame
		warn('pressed')
		local hrp = Find(name)
		local promt = FindPromt(name)
		game.Players.LocalPlayer.HumanoidRootPart.CFrame = hrp
		fireproximityprompt(promt)
		task.wait(1)
		game.Players.LocalPlayer.HumanoidRootPart.CFrame = olspos
	end
